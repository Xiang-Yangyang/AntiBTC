# Deployment account private key (Please replace with your own private key)
PRIVATE_KEY=your_private_key_here

# BSC Testnet RPC URL
BSC_TESTNET_URL=https://data-seed-prebsc-1-s1.binance.org:8545/

# BSC Mainnet RPC URL
BSC_MAINNET_URL=https://bsc-dataseed.binance.org/

# Initial BTC price (in USD, 8 decimal places)
INITIAL_BTC_PRICE=2000000000000

# Whether to verify contract (true/false)
VERIFY_CONTRACT=false 

# Oracle configuration
# Set to true to use Chainlink oracle, false to use Mock oracle
USE_CHAINLINK=false

# BSCScan API Key
BSCSCAN_API_KEY=your_bscscan_api_key(optional, for contract verification) 